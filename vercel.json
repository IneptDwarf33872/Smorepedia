{
    "functions": {
        "api/index.js": { "runtime": "@vercel/node@20.11.1" }
      },
    "routes": [
      {
        "src": "/redirect",
        "status": 308,
        "headers": { "Location": "https://example.com/" }
      },
      {
        "src": "/custom-page",
        "headers": { "cache-control": "s-maxage=1000" },
        "dest": "/index.html"
      },
      { "src": "/api", "dest": "api/index.js" }
    ]
  }